{
  "name": "garoon-dts",
  "version": "0.0.1",
  "description": "Type definitions for garoon JavaScript customize",
  "main": "index.js",
  "scripts": {
    "test": "run-s prettier:check tsc",
    "tsc": "tsc",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --check \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garoon/garoon-dts.git"
  },
  "keywords": [
    "garoon",
    "TypeScript",
    "cybozu"
  ],
  "author": "Cybozu.inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garoon/garoon-dts/issues"
  },
  "homepage": "https://github.com/garoon/garoon-dts#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3333"
  }
}
